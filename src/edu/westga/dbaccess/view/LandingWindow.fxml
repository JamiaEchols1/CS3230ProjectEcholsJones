<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="287.0" prefWidth="364.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.dbaccess.view.LandingWindowCodeBehind">
   <children>
      <AnchorPane minWidth="450.0" prefHeight="287.0" prefWidth="498.0">
         <children>
            <Label fx:id="employeeNameLbl" layoutX="8.0" layoutY="14.0" prefHeight="17.0" prefWidth="126.0" />
            <Label fx:id="usernameLbl" layoutX="130.0" layoutY="14.0" prefHeight="17.0" prefWidth="126.0" />
            <Label fx:id="idLbl" layoutX="292.0" layoutY="14.0" prefHeight="17.0" prefWidth="60.0" />
            <Button fx:id="registerBtn" layoutX="301.0" layoutY="94.0" mnemonicParsing="false" onAction="#registerBtnClick" text="Register" />
            <Button fx:id="shopButton" disable="true" layoutX="14.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleShopClick" text="Shop for furniture" />
            <ComboBox fx:id="customerComboBox" layoutX="32.0" layoutY="94.0" prefWidth="150.0" promptText="SELECT CUSTOMER" />
            <Button fx:id="searchBtn" layoutX="217.0" layoutY="94.0" mnemonicParsing="false" onAction="#searchBtnClick" text="Search" />
            <Button fx:id="returnFurnitureButton" layoutX="154.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleReturnFurnitureClick" text="Return Furniture" />
            <Button fx:id="logoutButton" layoutX="352.0" layoutY="240.0" mnemonicParsing="false" onAction="#handleLogoutButtonClick" text="LOGOUT" />
            <Button fx:id="editProfileButton" layoutX="32.0" layoutY="240.0" mnemonicParsing="false" onAction="#handleEditProfileButtonClick" text="Edit Profile" />
            <Button fx:id="historyButton" layoutX="292.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleHistoryButtonClick" text="View Customer History" />
         </children>
      </AnchorPane>
   </children>
</VBox>
